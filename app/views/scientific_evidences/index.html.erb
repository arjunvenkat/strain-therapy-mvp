<p id="notice"><%= notice %></p>

<h1>Listing Scientific Evidences</h1>

<table>
  <thead>
    <tr>
      <th>Ailment</th>
      <th>Description</th>
      <th>Link</th>
      <th>Featured</th>
      <th>Comment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scientific_evidences.each do |scientific_evidence| %>
      <tr>
        <td><%= scientific_evidence.ailment_id %></td>
        <td><%= scientific_evidence.description %></td>
        <td><%= scientific_evidence.link %></td>
        <td><%= scientific_evidence.featured %></td>
        <td><%= scientific_evidence.comment %></td>
        <td><%= link_to 'Show', scientific_evidence %></td>
        <td><%= link_to 'Edit', edit_scientific_evidence_path(scientific_evidence) %></td>
        <td><%= link_to 'Destroy', scientific_evidence, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Scientific evidence', new_scientific_evidence_path %>
